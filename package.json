{
  "name": "angular-checklist",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"yarn build-content:watch\"",
    "build": "ng build",
    "build:ssr": "ng build --prod && ng run angular-checklist:server -- --output-hashing=none",
    "prerender": "npm run build:ssr && ts-node prerender.ts",
    "serve:ssr": "npm run prerender && http-server dist/browser",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-content": "ts-node ./tools/build-checklist.ts",
    "build-content:watch": "nodemon",
    "ci": "npm run build-content && ng build --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/cdk": "^6.0.2",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/material": "^6.0.2",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/platform-server": "^7.0.1",
    "@angular/router": "^7.0.1",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@ngrx/router-store": "^6.0.0-beta.3",
    "@ngrx/store": "^6.0.0-beta.3",
    "@ngrx/store-devtools": "^6.0.0-beta.3",
    "@types/node": "^10.1.2",
    "core-js": "^2.5.4",
    "fuzzysort": "^1.1.4",
    "highlight.js": "^9.12.0",
    "ngrx-store-localstorage": "^5.0.0",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.3",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/language-service": "^7.0.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@types/highlight.js": "^9.12.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/markdown-it": "^0.0.4",
    "chalk": "^2.4.1",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.0.1",
    "gray-matter": "^4.0.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "markdown-it": "^8.4.1",
    "ngrx-store-freeze": "^0.2.3",
    "nodemon": "^1.18.4",
    "protractor": "~5.3.0",
    "shorthash": "^0.0.2",
    "ts-node": "^6.0.3",
    "tslint": "~5.11.0",
    "typescript": "~3.1.4"
  }
